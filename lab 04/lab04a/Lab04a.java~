import java.util.Scanner;

/**
 * __Lab04a___ 
 * @onuralp avc�___
 * @29.10.2019__
 */ 
public class Lab04a
{
   public static void main( String[] args)
   {
      Scanner scan = new Scanner( System.in);

      // constants

      // variables
      int n;
      int zero;
      int loop;
      double decimal;

      // program code
      System.out.println( "Start... Enter a number:");
      n = scan.nextInt();
      zero = 0;
      loop = 0;
      if(n < 0){
         System.out.println("Please enter a positive value!");
      }
      else{
         // 1. Loop
         while(zero < n-1){
            System.out.print(zero + " ");
            zero += 1;
         }
         while(zero == n-1){
            System.out.println(zero + " ");
            zero += 1;
         }
         zero = 0;

         // 2. Loop
         while(zero < n){
            if((zero+1) % 5 != 0){
               System.out.print(zero + " ");
               zero += 1;
            }
            else{
               System.out.println(zero);
               zero += 1;
            }
         }
         zero = 0;
         System.out.println("");

         // 3. Loop
         while(zero < n){
            System.out.print(n-zero + " ");
            zero += 1;
         }
         while(zero == n){
            System.out.println(n-zero + " ");
            zero += 1;
         }
         zero = 0;

         // 4. Loop
         while(zero < (2*n)){
            System.out.print((zero-n) + " ");
            zero += 1;
         }
         while(zero == (2*n)){
            System.out.println((zero-n) + " ");
            zero += 1;
         }
         zero = 0;

         // 5. Loop
         while(zero < n+1){
            if((zero+2) % 10 != 0){
               if(zero % 2 == 0){
                  System.out.print(zero*zero + " ");
                  zero += 1;
               }
               else{
                  zero += 1;
               }
            }
            else{
               if(zero % 2 == 0){
                   System.out.println(zero*zero + " ");
                   zero += 1;
               }
            }
         }
         zero = 0;
         System.out.println("");

         // 6. Loop
         while((n-zero) > 2){
            if((loop+1) % 5 != 0){
               if((n-zero) % 3 == 0 ^ (n-zero) % 4 == 0){
                  System.out.print(n-zero + " ");
                  zero += 1;
                  loop += 1;
               }
               else{
                  zero += 1;
               }
            }
            else{
               if((n-zero) % 3 == 0 ^ (n-zero) % 4 == 0){
                  System.out.println(n-zero + " ");
                  zero += 1;
                  loop += 1;
               }
               else{
                  zero += 1;
               }
            }
         }
         zero = 0;
         loop = 0;
         System.out.println("");

         // 7. Loop
         while((n-zero) > 1){
            if(n % (n - zero) == 0){
               System.out.print(n - zero + " ");
               zero += 1;
            }
            else{
               zero += 1;
            }
         }
         zero = 0;
         System.out.println("");

         // 8. Loop
         while(n - zero < 100 && zero < n){
            decimal = Math.round(1.0 / (n - zero)*100)/100.0;
            System.out.print(decimal + " ");
            zero += 1;
         }
         zero = 0;
         System.out.println("");
      }
      System.out.println( "End.");
   }
}

